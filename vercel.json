{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|favicon\\.svg|logo-bb\\.svg|manifest\\.json|robots\\.txt|sitemap\\.xml).*)",
      "destination": "/index.html"
    }
  ]
}
